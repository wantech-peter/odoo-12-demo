<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="paper_format_payment_receipt" model="report.paperformat">
            <field name="name">US Letter</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">6</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">45</field>
            <field name="dpi">80</field>
        </record>

        <report
                id="action_picking_list_report"
                model="report.test.stock.forecast"
                string="執貨單(客戶明細)"
                report_type="qweb-pdf"
                name="picking_list_report.report_list_picking"
                file="picking_list_report.report_list_picking"
        />


        <report
                id="action_picking_list_report_pk"
                model="report.test.stock.forecast"
                string="執貨單(百佳明細)"
                report_type="qweb-pdf"
                name="picking_list_report.report_list_picking_pk"
                file="picking_list_report.report_list_picking_pk"
        />

        <report
                id="action_picking_list_report_kb"
                model="report.test.stock.forecast"
                string="執貨單(佳寶明細)"
                report_type="qweb-pdf"
                name="picking_list_report.report_list_picking_kb"
                file="picking_list_report.report_list_picking_kb"
        />

        <report
                id="action_picking_list_report3"
                model="report.test.stock.forecast"
                string="執貨單(大昌明細)"
                report_type="qweb-pdf"
                name="picking_list_report.report_list_picking3"
                file="picking_list_report.report_list_picking3"
        />

        <report
                id="report_test_stock_forecast_car"
                model="report.test.stock.forecast.car"
                string="Picking List (car number)"
                report_type="qweb-pdf"
                name="picking_list_report.report_list_picking_car_number"
                file="picking_list_report.report_list_picking_car_number"
        />

        <report
                id="action_product_list_report_category"
                model="report.test.stock.forecast.car"
                string="執貨總表"
                report_type="qweb-pdf"
                paperformat="picking_list_report.paper_format_payment_receipt"
                name="picking_list_report.report_list_product_category_car"
                file="picking_list_report.report_list_product_category_car"
        />


