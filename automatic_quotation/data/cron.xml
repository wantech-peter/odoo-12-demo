<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Cron Job For Generating Quotations-->

        <record id="ir_cron_generate_quotation" model="ir.cron">
            <field name="name">Generate Quotations</field>
            <field name="model_id" ref="model_quotation_auto_generate"/>
            <field name="state">code</field>
            <field name="code">model.auto_generate_quotations()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>
