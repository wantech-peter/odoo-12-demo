<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="sales_order_list_receipt.report_saleorder">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="sales_order_list_receipt.receipt_list_sales_order" t-lang="doc.partner_id.lang"/>
                <t t-if="doc.print_receipt">
                    <t t-call="sales_order_list_receipt.receipt_list_sales_order" t-lang="doc.partner_id.lang"/>
                </t>
            </t>
    </t>
</template>
</odoo>