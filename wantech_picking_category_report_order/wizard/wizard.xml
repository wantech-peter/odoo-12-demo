<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="picking_car_favourite_wizard_form" model="ir.ui.view">
            <field name="name">picking.car.favourite.wizard.form</field>
            <field name="model">picking.car.favourite.wizard</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="filter_id" widget="selection"/>
                            </group>
                            <group>
                                <field name="sort_by"/>
                            </group>
                        </group>
                        <footer>
                            <button name="print_report" string="Print" type="object" class="btn btn-primary"/>
                            <button name="cancel" string="Cancel" type="object" special="cancel" class="btn btn-secondary"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>