<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paper_format_followup_consolidated" model="report.paperformat">
            <field name="name">US Letter Followup Consolidated</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="margin_bottom">37</field>
        </record>
        <report
            id="action_followup_consolidated_report"
            model="report.statement"
            string="Consolidated Statement"
            report_type="qweb-pdf"
            paperformat="statement_report_consolidated.paper_format_followup_consolidated"
            name="statement_report_consolidated.followup_consolidated_report"
            file="statement_report_consolidated.followup_consolidated_report"
        />

    </data>
</odoo>
