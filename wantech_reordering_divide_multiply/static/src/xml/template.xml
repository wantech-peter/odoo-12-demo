<?xml version="1.0" encoding="utf-8"?>
<templates t-name="selectcolumns">
    <t t-extend="ListView.buttons">
        <t t-jquery=".o_list_buttons" t-operation="prepend">
            <t t-if="widget.modelName == 'stock.warehouse.orderpoint'">
                <div class="btn-group">
                    <button class="btn reod_prod_qty_updt_mul">Multiply <span t-esc="widget.model.modelName"/></button>
                </div>
            </t>
        </t>
    </t>
    <t t-extend="ListView.buttons">
        <t t-jquery=".o_list_buttons" t-operation="prepend">
            <t t-if="widget.modelName == 'stock.warehouse.orderpoint'">
                <div class="btn-group">
                    <button class="btn reod_prod_qty_updt_div">Divide</button>
                </div>
            </t>
        </t>
    </t>
    <t t-extend="ListView.buttons">
        <t t-jquery=".o_list_buttons" t-operation="prepend">
            <t t-if="widget.modelName == 'stock.warehouse.orderpoint'">
                <div class="btn-group hidden-xs o_dropdown">
                    <input type="text"
                           class="o_field_char o_field_widget o_input o_input_quant_multiple"
                           placeholder="1"/>
                </div>
            </t>
        </t>
    </t>
</templates>
