<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="paper_format_cutoff_receipt" model="report.paperformat">
            <field name="name">US Letter Cutoff Receipt</field>
            <field name="default" eval="True"/>
            <field name="format">A5</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">7</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">45</field>
            <field name="dpi">92</field>
        </record>
        <report
                id="action_report_cutoff_receipt"
                print_report_name="'Receipt (Month End+10)'"
                string="Print Receipt (Month End+10)"
                model="report.statement.cutoff"
                report_type="qweb-pdf"
                file="statement_report_cutoff_receipt.report_cutoff_receipt"
                name="statement_report_cutoff_receipt.report_cutoff_receipt"
                paperformat="statement_report_cutoff_receipt.paper_format_cutoff_receipt"
                menu="True"
        />

    </data>
</odoo>